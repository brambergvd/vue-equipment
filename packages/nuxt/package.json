{
  "name": "nuxt",
  "version": "0.6.1",
  "devDependencies": {
    "@nuxt/devtools": "0.7.4",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@unocss/nuxt": "^0.55.0",
    "nuxt": "^3.7.1",
    "vitest": "^0.34.1"
  },
  "scripts": {
    "build": "nlx nuxi prepare ./playground && nuxt-module-build --outDir ../../dist/nuxt",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@nuxt/kit": "^3.7.1"
  }
}
