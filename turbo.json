{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**"]
    },
    "build:types": {
      "cache": false,
      "outputs": ["./types/**"]
    },
    "//#metadata:update": {
      "cache": false,
      "outputs": ["./packages/metadata/index.json"]
    },
    "//#t:release": {
      "dependsOn": ["^build"]
    },
    "//#t:docs:dev": {
      "dependsOn": ["^build", "^build:types", "//#metadata:update"]
    },
    "//#t:docs:build": {
      "dependsOn": ["^build", "^build:types", "//#metadata:update"]
    },
    "nuxt#build": {
      "dependsOn": ["composables#build", "plugins#build", "//#metadata:update"]
    },
    "nuxt#dev": {
      "dependsOn": ["composables#build", "plugins#build"]
    }
  }
}
