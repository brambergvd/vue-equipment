{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "//#update:metadata": {
      "cache": false,
      "outputs": ["./packages/metadata/index.json"]
    },
    "//#t:release": {
      "cache": false,
      "dependsOn": [
        "^build:types",
        "composables#build",
        "plugins#build",
        "nuxt#build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["//#update:metadata", "build:types", "^build"]
    },
    "nuxt#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["//#update:metadata", "composables#build", "plugins#build"]
    },
    "build": {
      "outputs": ["./dist/**"]
    },
    "build:types": {
      "cache": false,
      "outputs": ["./types/**"]
    },
    "docs#build": {
      "cache": false,
      "dependsOn": ["//#update:metadata", "^build:types", "^build"]
    },
    "plugins#build": {
      "dependsOn": ["utils#build"]
    },
    "nuxt#build": {
      "dependsOn": ["//#update:metadata", "composables#build", "plugins#build"]
    }
  }
}
