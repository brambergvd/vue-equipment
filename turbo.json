{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**"]
    },
    "build:types": {
      "outputs": ["./types/**"]
    },
    "//#release-it": {
      "dependsOn": ["^build"]
    },
    "nuxt#build": {
      "dependsOn": ["composables#build", "plugins#build"]
    },
    "nuxt#dev": {
      "dependsOn": ["composables#build", "plugins#build"]
    }
  }
}
