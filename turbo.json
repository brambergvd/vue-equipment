{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["packages/metadata/index.json"],
  "pipeline": {
    "build": {
      "cache": false,
      "outputs": ["./dist/**"],
      "dependsOn": ["//#metadata:update"]
    },
    "build:types": {
      "cache": false,
      "outputs": ["./types/**"]
    },
    "//#metadata:update": {
      "cache": false,
      "outputs": ["./packages/metadata/index.json"]
    },
    "//#t:release": {
      "dependsOn": ["^build", "nuxt#build", "^build:types"]
    },
    "//#t:docs:dev": {
      "cache": false,
      "dependsOn": ["//#metadata:update", "^build:types", "^build"]
    },
    "//#t:docs:build": {
      "dependsOn": ["//#metadata:update", "^build:types", "^build"]
    },
    "plugins#build": {
      "dependsOn": ["utils#build"]
    },
    "nuxt#build": {
      "dependsOn": ["//#metadata:update", "composables#build", "plugins#build"]
    },
    "nuxt#dev": {
      "dependsOn": ["//#metadata:update", "composables#build", "plugins#build"]
    }
  }
}
