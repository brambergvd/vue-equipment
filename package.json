{
  "name": "@maas/vue-equipment",
  "description": "A magic collection of Vue composables, plugins, components and directives",
  "version": "0.12.0",
  "author": "Robin Scholz <https://github.com/robinscholz>, Christoph Jeworutzki <https://github.com/ChristophJeworutzki>",
  "devDependencies": {
    "@antfu/ni": "^0.21.5",
    "@nuxt/module-builder": "^0.4.0",
    "@release-it/bumper": "^5.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/tsconfig": "^0.4.0",
    "composables": "workspace:*",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "metadata": "workspace:*",
    "plugins": "workspace:*",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unocss": "^0.54.1",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue": "4.2.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.8",
    "vitepress": "1.0.0-beta.7",
    "vue": "^3.3.4"
  },
  "exports": {
    ".": {
      "types": "./dist/nuxt/types.d.ts",
      "require": "./dist/nuxt/module.cjs",
      "import": "./dist/nuxt/module.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "require": "./dist/composables/index.js",
      "import": "./dist/composables/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.mjs"
    },
    "./utils/css/*.css": {
      "import": "./dist/utils/css/*.css",
      "require": "./dist/utils/css/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "./dist/nuxt/module.cjs",
  "peerDependencies": {
    "@maas/magic-timer": "^1.0.0",
    "@vueuse/core": "^10.3.0",
    "@vueuse/integrations": "^10.3.0",
    "focus-trap": "^7.5.2",
    "hls.js": "^1.4.10",
    "luxon": "^3.4.2",
    "mitt": "^3.0.1",
    "motion": "^10.16.2",
    "nuxt": "^3.5.1",
    "universal-cookie": "^6.1.1"
  },
  "peerDependenciesMeta": {
    "@maas/magic-timer": {
      "optional": false
    },
    "@vueuse/core": {
      "optional": false
    },
    "@vueuse/integrations": {
      "optional": false
    },
    "hls.js": {
      "optional": false
    },
    "luxon": {
      "optional": false
    },
    "mitt": {
      "optional": false
    },
    "motion": {
      "optional": false
    },
    "nuxt": {
      "optional": false
    },
    "universal-cookie": {
      "optional": false
    }
  },
  "scripts": {
    "dev": "nr dev:docs",
    "dev:docs": "turbo run t:docs:dev && vitepress dev packages",
    "dev:nuxt": "turbo run dev --filter=nuxt",
    "build": "turbo run build",
    "build:types": "turbo run build:types",
    "docs:build": "turbo run t:docs:build && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "metadata:update": "nr -C packages/metadata update",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uninstall": "nlx rimraf --glob ./**/node_modules --glob ./**/pnpm-lock.yaml",
    "release": "turbo run t:release && release-it"
  },
  "types": "./dist/nuxt/types.d.ts",
  "volta": {
    "node": "18.16.1"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true
    },
    "npm": {
      "allowSameVersion": true
    },
    "plugins": {
      "@release-it/bumper": {
        "in": "package.json",
        "out": [
          "package.json",
          "packages/*/package.json"
        ]
      }
    }
  }
}
