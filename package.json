{
  "name": "@maas/vue-equipment",
  "version": "0.0.1",
  "private": true,
  "description": "A magic collection of Vue composables, plugins, components and directives",
  "scripts": {
    "build": "nr metadata:update && pnpm run clean && pnpm run build:composables && pnpm run build:plugins && pnpm run build:nuxt && pnpm run build:types",
    "build:nuxt": "nuxt-module-build ./packages/nuxt --outDir ../../dist/nuxt",
    "build:plugins": "esno scripts/plugins.ts",
    "build:composables": "tsup --config  packages/composables/tsup.config.ts",
    "build:types": "vue-tsc --project tsconfig.types.json --declaration --emitDeclarationOnly",
    "clean": "pnpm dlx rimraf dist types packages/*/dist",
    "dev": "nr metadata:update && nr docs:dev",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "module:dev": "cd packages/nuxt && nr dev",
    "metadata:update": "nr -C packages/metadata update",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uninstall": "pnpm dlx rimraf --glob ./**/node_modules --glob ./**/pnpm-lock.yaml"
  },
  "keywords": [],
  "license": "MIT",
  "author": "Robin Scholz <https://github.com/robinscholz>, Christoph Jeworutzki <https://github.com/ChristophJeworutzki>",
  "exports": {
    "./composables": {
      "types": "./composables/index.d.ts",
      "require": "./composables/index.js",
      "import": "./composables/index.mjs"
    },
    "./plugins": {
      "types": "./plugins/index.d.ts",
      "require": "./plugins/index.js",
      "import": "./plugins/index.mjs"
    },
    "./nuxt": {
      "types": "./nuxt/module.d.ts",
      "require": "./nuxt/module.cjs",
      "import": "./nuxt/module.mjs"
    }
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@nuxt/module-builder": "^0.4.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.1.0",
    "composables": "workspace:*",
    "consola": "^3.1.0",
    "eslint": "^8.39.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "metadata": "workspace:*",
    "mkdist": "^1.2.0",
    "plugins": "workspace:*",
    "prettier": "^2.8.8",
    "simple-git": "^3.17.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "unocss": "^0.51.8",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue": "4.2.2-beta.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.1",
    "vitepress": "1.0.0-alpha.73",
    "vue": "^3.2.47",
    "vue-tsc": "^1.8.3"
  },
  "volta": {
    "node": "18.16.1"
  }
}
