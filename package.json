{
  "name": "@maas/vue-equipment",
  "description": "A magic collection of Vue composables, plugins, components and directives",
  "version": "0.40.0",
  "contributors": [
    {
      "name": "Robin Scholz",
      "url": "https://github.com/robinscholz"
    },
    {
      "name": "Christoph Jeworutzki",
      "url": "https://github.com/ChristophJeworutzki"
    }
  ],
  "type": "module",
  "devDependencies": {
    "@antfu/ni": "^0.23.2",
    "@maas/config": "^1.4.1",
    "@release-it/bumper": "^6.0.1",
    "@types/node": "catalog:",
    "release-it": "^17.11.0",
    "tailwindcss": "catalog:",
    "turbo": "^2.3.3",
    "typescript": "catalog:"
  },
  "exports": {
    "./nuxt": {
      "types": "./dist/nuxt/types.d.mts",
      "require": "./dist/nuxt/module.cjs",
      "import": "./dist/nuxt/module.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.mts",
      "require": "./dist/composables/index.js",
      "import": "./dist/composables/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.mjs"
    },
    "./utils/css/*.css": {
      "import": "./dist/utils/css/*.css",
      "require": "./dist/utils/css/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "@floating-ui/vue": "catalog:",
    "@maas/magic-timer": "catalog:",
    "@maas/vue-primitive": "catalog:",
    "@maas/wheel-gestures": "catalog:",
    "@nuxt/kit": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/integrations": "catalog:",
    "defu": "catalog:",
    "focus-trap": "catalog:",
    "hls.js": "catalog:",
    "luxon": "catalog:",
    "mitt": "catalog:",
    "motion": "catalog:",
    "nuxt": "catalog:",
    "universal-cookie": "catalog:",
    "vue": "catalog:"
  },
  "peerDependenciesMeta": {
    "@floating-ui/vue": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@maas/magic-timer": {
      "optional": true
    },
    "@maas/vue-primitive": {
      "optional": true
    },
    "@maas/wheel-gestures": {
      "optional": true
    },
    "@vueuse/core": {
      "optional": true
    },
    "@vueuse/integrations": {
      "optional": true
    },
    "defu": {
      "optional": true
    },
    "hls.js": {
      "optional": true
    },
    "luxon": {
      "optional": true
    },
    "mitt": {
      "optional": true
    },
    "motion": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "universal-cookie": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:docs": "turbo run dev --filter=docs",
    "dev:nuxt": "turbo run dev --filter=nuxt",
    "build": "turbo run build --filter=!playground",
    "build:types": "turbo run build:types",
    "build:docs": "turbo run build --filter=docs",
    "preview:docs": "turbo run preview --filter=docs",
    "update:metadata": "nr -C packages/metadata update",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "turbo run lint",
    "uninstall": "pnpm dlx rimraf --glob ./**/node_modules --glob ./**/pnpm-lock.yaml",
    "release": "turbo run t:release && release-it",
    "update:deps": "pnpm dlx taze -r -w",
    "update:deps:major": "pnpm dlx taze major -r -w"
  },
  "volta": {
    "node": "20.18.1"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true
    },
    "npm": {
      "allowSameVersion": true
    },
    "plugins": {
      "@release-it/bumper": {
        "in": "package.json",
        "out": [
          "package.json",
          "packages/*/package.json",
          "apps/*/package.json"
        ]
      }
    }
  },
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "overrides": {
      "typescript": "catalog:"
    }
  }
}
