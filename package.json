{
  "name": "@magicasaservice/vue-equipment",
  "version": "0.0.1",
  "private": true,
  "description": "A magic collection of Vue composables, plugins, components and directives",
  "scripts": {
    "build": "unbuild",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist types packages/*/dist",
    "dev": "nr update && nr docs:dev",
    "docs:dev": "vitepress dev packages --open",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "nr -C packages/metadata update"
  },
  "keywords": [],
  "license": "MIT",
  "author": "Robin Scholz <https://github.com/robinscholz>, Christoph Jeworutzki <https://github.com/ChristophJeworutzki>",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.mjs"
    },
    "./*": "./*",
    "./composables": {
      "types": "./composables.d.ts",
      "require": "./composables.cjs",
      "import": "./composables.mjs"
    },
    "./plugins": {
      "types": "./plugins.d.ts",
      "require": "./plugins.cjs",
      "import": "./plugins.mjs"
    }
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@rollup/plugin-json": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "@vueuse/core": "^10.1.0",
    "composables": "workspace:*",
    "consola": "^3.1.0",
    "esbuild": "^0.17.18",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.39.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "metadata": "workspace:*",
    "plugins": "workspace:*",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-pure": "^0.0.2",
    "simple-git": "^3.17.0",
    "tsx": "^3.12.6",
    "unbuild": "^1.2.1",
    "unocss": "^0.51.8",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue": "^4.1.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.1",
    "vitepress": "1.0.0-alpha.73",
    "vue": "^3.2.47"
  }
}
