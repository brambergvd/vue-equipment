{
  "name": "@maas/vue-equipment",
  "description": "A magic collection of Vue composables, plugins, components and directives",
  "version": "0.1.0",
  "author": "Robin Scholz <https://github.com/robinscholz>, Christoph Jeworutzki <https://github.com/ChristophJeworutzki>",
  "devDependencies": {
    "@antfu/ni": "^0.21.5",
    "@nuxt/module-builder": "^0.4.0",
    "@release-it/bumper": "^5.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "@types/prettier": "^3.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/tsconfig": "^0.4.0",
    "composables": "workspace:*",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "metadata": "workspace:*",
    "plugins": "workspace:*",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "syncpack": "^10.9.3",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unocss": "^0.54.1",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue": "4.2.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.8",
    "vitepress": "1.0.0-beta.7",
    "vue": "^3.2.47"
  },
  "exports": {
    ".": {
      "types": "./dist/nuxt/types.d.ts",
      "require": "./dist/nuxt/module.cjs",
      "import": "./dist/nuxt/module.mjs"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "require": "./dist/composables/index.js",
      "import": "./dist/composables/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "./dist/nuxt/module.cjs",
  "peerDependencies": {
    "@vueuse/core": "^10.3.0",
    "@vueuse/integrations": "10.3.0",
    "@vueuse/shared": "10.3.0",
    "focus-trap": "7.5.2",
    "hls.js": "1.4.10",
    "mitt": "3.0.1",
    "nuxt": "3.x",
    "uuid": "9.0.0"
  },
  "peerDependenciesMeta": {
    "@vueuse/core": {
      "optional": true
    },
    "@vueuse/integrations": {
      "optional": true
    },
    "@vueuse/shared": {
      "optional": true
    },
    "hls.js": {
      "optional": true
    },
    "mitt": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "uuid": {
      "optional": true
    }
  },
  "scripts": {
    "build": "nr clean && nr metadata:update && turbo run build",
    "clean": "nlx rimraf dist types packages/*/dist",
    "dev": "nr metadata:update && nr docs:dev",
    "dev:nuxt": "turbo run dev --filter=nuxt",
    "docs:build": "vitepress build packages",
    "docs:dev": "vitepress dev packages",
    "docs:preview": "vitepress preview packages",
    "metadata:update": "nr -C packages/metadata update",
    "module:dev": "cd packages/nuxt && nr dev",
    "prepare": "nr build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uninstall": "nlx rimraf --glob ./**/node_modules --glob ./**/pnpm-lock.yaml",
    "release": "turbo run release --concurrency=1 && release-it"
  },
  "types": "./dist/nuxt/types.d.ts",
  "volta": {
    "node": "18.16.1"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false
    },
    "npm": {
      "allowSameVersion": true
    },
    "plugins": {
      "@release-it/bumper": {
        "in": "package.json",
        "out": [
          "package.json",
          "packages/*/package.json"
        ]
      }
    }
  }
}
